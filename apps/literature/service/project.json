{
	"name": "literature-service",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/literature/service/src",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nx/rspack:rspack",
			"outputs": [ "{options.outputPath}" ],
			"defaultConfiguration": "production",
			"options": {
				"target": "node",
				"outputPath": "apps/literature/service/dist",
				"main": "apps/literature/service/src/main.ts",
				"tsConfig": "apps/literature/service/tsconfig.build.json",
				"assets": [ "apps/literature/service/src/proto" ],
				"rspackConfig": "rspack.config.js"
			},
			"configurations": {
				"development": {
					"mode": "development"
				},
				"production": {
					"mode": "production",
					"sourceMap": false
				}
			}
		},
		"serve": {
			"executor": "@nx/js:node",
			"options": {
				"buildTarget": "literature-service:build",
				"inspect": false
			},
			"configurations": {
				"production": {
					"buildTarget": "literature-service:build:production"
				}
			}
		}
	},
	"tags": [ ]
}
