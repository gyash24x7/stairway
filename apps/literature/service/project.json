{
	"name": "literature-service",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/literature/service/src",
	"projectType": "application",
	"targets": {
		"proto:gen": {
			"command": ""
		},
		"build": {
			"executor": "@nrwl/webpack:webpack",
			"outputs": [ "{options.outputPath}" ],
			"options": {
				"target": "node",
				"compiler": "tsc",
				"outputPath": "apps/literature/service/dist",
				"main": "apps/literature/service/src/main.ts",
				"tsConfig": "apps/literature/service/tsconfig.build.json",
				"assets": [ "apps/literature/service/src/proto" ]
			},
			"configurations": {
				"production": {
					"optimization": true,
					"extractLicenses": true,
					"inspect": false
				}
			}
		},
		"serve": {
			"executor": "@nrwl/js:node",
			"options": {
				"buildTarget": "literature-service:build",
				"inspect": false
			},
			"configurations": {
				"production": {
					"buildTarget": "literature-service:build:production"
				}
			}
		}
	},
	"tags": [ ]
}
