{
	"name": "@s2h/web",
	"version": "1.0.0",
	"description": "Web Worker for S2H Application",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"start": "vite preview",
		"generate": "wrangler types ./env.d.ts --include-runtime=false",
		"migrate:new": "drizzle-kit generate",
		"migrate:dev": "wrangler d1 migrations apply DB --local",
		"migrate:prod": "wrangler d1 migrations apply DB --remote",
		"deploy": "wrangler deploy"
	},
	"dependencies": {
		"@s2h-ui/auth": "workspace:*",
		"@s2h-ui/callbreak": "workspace:*",
		"@s2h-ui/fish": "workspace:*",
		"@s2h-ui/primitives": "workspace:*",
		"@s2h-ui/shared": "workspace:*",
		"@s2h-ui/splendor": "workspace:*",
		"@s2h-ui/wordle": "workspace:*",
		"@s2h/auth": "workspace:*",
		"@s2h/callbreak": "workspace:*",
		"@s2h/client": "workspace:*",
		"@s2h/fish": "workspace:*",
		"@s2h/splendor": "workspace:*",
		"@s2h/utils": "workspace:*",
		"@s2h/wordle": "workspace:*",
		"@tanstack/react-router": "1.144.0",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"tailwindcss": "4.1.18",
		"tw-animate-css": "1.4.0"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "1.19.0",
		"@tailwindcss/vite": "4.1.18",
		"@types/react": "19.2.7",
		"@types/react-dom": "19.2.3",
		"@vitejs/plugin-react-swc": "4.2.2",
		"drizzle-kit": "0.31.8",
		"vite": "7.3.0",
		"vite-plugin-svgr": "4.5.0",
		"wrangler": "4.56.0"
	}
}
