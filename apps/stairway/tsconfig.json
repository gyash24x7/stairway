{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": [ "expo/tsconfig.base", "@tsconfig/strictest" ],
	"compilerOptions": {
		"exactOptionalPropertyTypes": false,
		"noUncheckedIndexedAccess": false,
		"paths": {
			"@/*": [ "./src/*" ]
		}
	},
	"include": [ "**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts" ]
}
