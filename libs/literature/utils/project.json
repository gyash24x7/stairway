{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/literature/utils/src",
  "projectType": "library",
  "targets": {
	"build": {
	  "executor": "@nrwl/js:tsc",
	  "outputs": [
		"{options.outputPath}"
	  ],
	  "options": {
		"outputPath": "dist/libs/literature/utils",
		"main": "libs/literature/utils/src/index.ts",
		"tsConfig": "libs/literature/utils/tsconfig.lib.json",
		"assets": [
		  "libs/literature/utils/*.md"
		]
	  }
	},
	"test": {
	  "executor": "@nrwl/jest:jest",
	  "outputs": [
		"coverage/libs/literature/utils"
	  ],
	  "options": {
		"jestConfig": "libs/literature/utils/jest.config.ts",
		"passWithNoTests": true
	  }
	}
  },
  "tags": [ ]
}
